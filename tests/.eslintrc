{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "spaced-comment": "off",
    "key-spacing": [
      "error",
      {
        "afterColon": true
      }
    ],
    "quotes": [
      "error",
      "double"
    ],
    "quote-props": "off",
    "camelcase": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-unused-expressions": "off",
    "@typescript-eslint/type-annotation-spacing": [
      "error",
      {
        "after": false
      }
    ],
    "@typescript-eslint/explicit-function-return-type": "error",
    "space-before-function-paren": [
      "error",
      "never"
    ],
    "eqeqeq": "off",
    "no-empty": "off",
    "no-useless-return": "off",
    "no-unreachable": "off",
    "prefer-const": "off",
    "space-in-parens": [
      "error",
      "never"
    ]
  },
  "ignorePatterns": [
    "./build",
    "./external/*",
    "./util/*"
  ]
}
